<?xml version="1.0"?>
<!DOCTYPE simulator [
<!ATTLIST algorithm id ID #IMPLIED>
<!ATTLIST problem id ID #IMPLIED>
<!ATTLIST measurements id ID #IMPLIED>
]>
<simulator>
    <algorithms>
        <algorithm id="fss_rosenbrock" class="fss.FSS">
            <initialisationStrategy class="algorithm.initialisation.ClonedPopulationInitialisationStrategy">
                <entityType class="fss.fish.StandardFish">
                    <maxWeight class="controlparameter.ConstantControlParameter" parameter="5000"/>
                    <individualMovement class="fss.movement.individual.StandardIndividualMovementProvider">
                        <individualStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="0.06" finalValue="0.0006"/>
                    </individualMovement>
                    <positionInitializationStrategy class="entity.initialization.RandomBoundedInitializationStrategy">
                        <lowerBound class="controlparameter.ConstantControlParameter" parameter="15"/>
                        <upperBound class="controlparameter.ConstantControlParameter" parameter="30"/>
                    </positionInitializationStrategy>
                </entityType>
                <entityNumber value="30"/>
            </initialisationStrategy>
            <iterationStrategy class="fss.iterationstrategies.FSSIterationStrategy">
                <volitiveMovement class="fss.movement.volitive.StandardVolitiveMovementProvider">
                    <volitiveStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="0.6" finalValue="0.006"/>
                </volitiveMovement>
            </iterationStrategy>
            <addStoppingCondition class="stoppingcondition.MeasuredStoppingCondition" target="10000"/>
        </algorithm>
        
        <algorithm id="fss_rastrigin" class="fss.FSS">
            <initialisationStrategy class="algorithm.initialisation.ClonedPopulationInitialisationStrategy">
                <entityType class="fss.fish.StandardFish">
                    <maxWeight class="controlparameter.ConstantControlParameter" parameter="5000"/>
                    <individualMovement class="fss.movement.individual.StandardIndividualMovementProvider">
                        <individualStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="0.01024" finalValue="0.0001024"/>
                    </individualMovement>
                    <positionInitializationStrategy class="entity.initialization.RandomBoundedInitializationStrategy">
                        <lowerBound class="controlparameter.DomainProportionalControlParameter" proportion="0.25"/>
                        <upperBound class="controlparameter.DomainProportionalControlParameter" proportion="0.5"/>
                    </positionInitializationStrategy>
                </entityType>
                <entityNumber value="30"/>
            </initialisationStrategy>
            <iterationStrategy class="fss.iterationstrategies.FSSIterationStrategy">
                <volitiveMovement class="fss.movement.volitive.StandardVolitiveMovementProvider">
                    <volitiveStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="0.1024" finalValue="0.001024"/>
                </volitiveMovement>
            </iterationStrategy>
            <addStoppingCondition class="stoppingcondition.MeasuredStoppingCondition" target="10000"/>
        </algorithm>
        
        <algorithm id="fss_griewank" class="fss.FSS">
            <initialisationStrategy class="algorithm.initialisation.ClonedPopulationInitialisationStrategy">
                <entityType class="fss.fish.StandardFish">
                    <maxWeight class="controlparameter.ConstantControlParameter" parameter="5000"/>
                    <individualMovement class="fss.movement.individual.StandardIndividualMovementProvider">
                        <individualStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="1.2" finalValue="0.012"/>
                    </individualMovement>
                    <positionInitializationStrategy class="entity.initialization.RandomBoundedInitializationStrategy">
                        <lowerBound class="controlparameter.DomainProportionalControlParameter" proportion="0.25"/>
                        <upperBound class="controlparameter.DomainProportionalControlParameter" proportion="0.5"/>
                    </positionInitializationStrategy>
                </entityType>
                <entityNumber value="30"/>
            </initialisationStrategy>
            <iterationStrategy class="fss.iterationstrategies.FSSIterationStrategy">
                <volitiveMovement class="fss.movement.volitive.StandardVolitiveMovementProvider">
                    <volitiveStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="12" finalValue="0.12"/>
                </volitiveMovement>
            </iterationStrategy>
            <addStoppingCondition class="stoppingcondition.MeasuredStoppingCondition" target="10000"/>
        </algorithm>
        
        <algorithm id="fss_ackley" class="fss.FSS">
            <initialisationStrategy class="algorithm.initialisation.ClonedPopulationInitialisationStrategy">
                <entityType class="fss.fish.StandardFish">
                    <maxWeight class="controlparameter.ConstantControlParameter" parameter="5000"/>
                    <individualMovement class="fss.movement.individual.StandardIndividualMovementProvider">
                        <individualStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="0.065536" finalValue="0.00065536"/>
                    </individualMovement>
                    <positionInitializationStrategy class="entity.initialization.RandomBoundedInitializationStrategy">
                        <lowerBound class="controlparameter.DomainProportionalControlParameter" proportion="0.25"/>
                        <upperBound class="controlparameter.DomainProportionalControlParameter" proportion="0.5"/>
                    </positionInitializationStrategy>
                </entityType>
                <entityNumber value="30"/>
            </initialisationStrategy>
            <iterationStrategy class="fss.iterationstrategies.FSSIterationStrategy">
                <volitiveMovement class="fss.movement.volitive.StandardVolitiveMovementProvider">
                    <volitiveStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="0.65536" finalValue="0.0065536"/>
                </volitiveMovement>
            </iterationStrategy>
            <addStoppingCondition class="stoppingcondition.MeasuredStoppingCondition" target="10000"/>
        </algorithm>
        
        <algorithm id="fss_schwefel_sphere" class="fss.FSS">
            <initialisationStrategy class="algorithm.initialisation.ClonedPopulationInitialisationStrategy">
                <entityType class="fss.fish.StandardFish">
                    <maxWeight class="controlparameter.ConstantControlParameter" parameter="5000"/>
                    <individualMovement class="fss.movement.individual.StandardIndividualMovementProvider">
                        <individualStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="0.2" finalValue="0.002"/>
                    </individualMovement>
                    <positionInitializationStrategy class="entity.initialization.RandomBoundedInitializationStrategy">
                        <lowerBound class="controlparameter.DomainProportionalControlParameter" proportion="0.25"/>
                        <upperBound class="controlparameter.DomainProportionalControlParameter" proportion="0.5"/>
                    </positionInitializationStrategy>
                </entityType>
                <entityNumber value="30"/>
            </initialisationStrategy>
            <iterationStrategy class="fss.iterationstrategies.FSSIterationStrategy">
                <volitiveMovement class="fss.movement.volitive.StandardVolitiveMovementProvider">
                    <volitiveStepSize class="controlparameter.LinearlyVaryingControlParameter" initialValue="2" finalValue="0.02"/>
                </volitiveMovement>
            </iterationStrategy>
            <addStoppingCondition class="stoppingcondition.MeasuredStoppingCondition" target="10000"/>
        </algorithm>
    </algorithms>

    <problems>
        <problem id="spherical" class="problem.FunctionOptimisationProblem" domain="R(-100:100)^30">
            <function class="functions.continuous.unconstrained.Spherical"/>
        </problem>
        <problem id="rosenbrock" class="problem.FunctionOptimisationProblem" domain="R(-30:30)^30">
            <function class="functions.continuous.unconstrained.Rosenbrock"/>
        </problem>
        <problem id="rastrigin" class="problem.FunctionOptimisationProblem" domain="R(-5.12:5.12)^30">
            <function class="functions.continuous.unconstrained.Rastrigin"/>
        </problem>
        <problem id="griewank" class="problem.FunctionOptimisationProblem" domain="R(-600:600)^30">
            <function class="functions.continuous.unconstrained.Griewank"/>
        </problem>
        <problem id="ackley" class="problem.FunctionOptimisationProblem" domain="R(-32.768:32.768)^30">
            <function class="functions.continuous.unconstrained.Ackley"/>
        </problem>
        <problem id="schwefel" class="problem.FunctionOptimisationProblem" domain="R(-100:100)^30">
            <function class="functions.continuous.SchwefelProblem1_2"/>
        </problem>
    </problems>
    
    <measurements id="fitness" class="simulator.MeasurementSuite" resolution="10">
        <addMeasurement class="measurement.single.fss.BarycenterFitness"/>
        <addMeasurement class="measurement.single.FitnessEvaluations"/>
    </measurements>
    
    <simulations>
        <simulation samples="30">
            <algorithm idref="fss_schwefel_sphere"/>
            <problem idref="spherical"/>
            <measurements idref="fitness" />
    	    <output format="TXT" file="data/spherical-fss-30.txt" />
        </simulation>
        <simulation samples="30">
            <algorithm idref="fss_rosenbrock"/>
            <problem idref="rosenbrock"/>
            <measurements idref="fitness" />
	        <output format="TXT" file="data/rosenbrock-fss-30.txt"/>
        </simulation>
        <simulation samples="30">
            <algorithm idref="fss_rastrigin"/>
            <problem idref="rastrigin"/>
            <measurements idref="fitness" />
	        <output format="TXT" file="data/rastrigin-fss-30.txt"/>
        </simulation>
        <simulation samples="30">
            <algorithm idref="fss_griewank"/>
            <problem idref="griewank"/>
            <measurements idref="fitness" />
	        <output format="TXT" file="data/griewank-fss-30.txt"/>
        </simulation>
        <simulation samples="30">
            <algorithm idref="fss_ackley"/>
            <problem idref="ackley"/>
            <measurements idref="fitness" />
	        <output format="TXT" file="data/ackley-fss-30.txt"/>
        </simulation>
        <simulation samples="30">
            <algorithm idref="fss_schwefel_sphere"/>
            <problem idref="schwefel"/>
            <measurements idref="fitness" />
    	    <output format="TXT" file="data/schwefelfss-30.txt"/>
        </simulation>
    </simulations>
</simulator>
